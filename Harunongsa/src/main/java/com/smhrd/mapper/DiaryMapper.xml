<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <!-- SQL문자와 Java코드를 분리하기 위한 파일 -->
<mapper namespace = "org.mybatis.example.DiaryMapper">
	<insert id="diary" parameterType="DiaryDTO">
		insert into diaries values(diaryidx.nextval, #{memberid}, #{cropidx}, #{diarytitle}, #{diarycontent}, #{diaryimg1}, #{diaryimg2}, sysdate)
	</insert>
</mapper>